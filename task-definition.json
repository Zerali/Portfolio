{
  "executionRoleArn": "arn:aws:iam::172419990402:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "portfolio",
      "image": "<image_name>",
      "essential": true,
      "portMappings": [{
        "hostPort": 80,
        "protocol": "tcp",
        "containerPort": 80
      }],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "awslogs-portfolio",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "awslogs-example"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "portfolio-deployment-task"
}